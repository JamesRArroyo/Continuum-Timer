{
  "manifest_version": 2,
  "name": "Gitlab Issue Timer",
  "description": "Timer for issues in Gitlab",
  "version": "1.0",
  "icons": {
    "16": "assets/logo/logo_16px.png",
    "32": "assets/logo/logo_32px.png",
    "48": "assets/logo/logo_48px.png",
    "128": "assets/logo/logo_128px.png",
    "256": "assets/logo/logo_256px.png",
    "512": "assets/logo/logo_512px.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "assets/logo/logo_16px.png",
      "32": "assets/logo/logo_32px.png",
      "48": "assets/logo/logo_48px.png",
      "128": "assets/logo/logo_128px.png",
      "256": "assets/logo/logo_256px.png",
      "512": "assets/logo/logo_512px.png"
    },
    "default_title": "Gitlab Issue Timer",
    "default_popup": "index.html"
  },
  "permissions": [],
  "content_security_policy":
    "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": ["assets/css/*", "assets/js/*", "assets/fonts/*"]
}
